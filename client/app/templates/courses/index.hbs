<div class="doc-content">
  {{#paper-content class="md-whiteframe-z1"}}
    {{#paper-list}}
      {{#paper-subheader class="md-no-sticky"}}Courses{{/paper-subheader}}
      {{#each model as |course|}}
      {{paper-divider}}
      {{#paper-item}}
        <p>{{course.name}}</p>
      {{/paper-item}}
      {{else}}
        {{#paper-item}}
          <p>Somethings not right here......</p>
        {{/paper-item}}
      {{/each}}
    {{/paper-list}}
  {{/paper-content}}
</div>
